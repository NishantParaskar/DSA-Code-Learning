import java.util.Arrays;
import java.util.TreeSet;
public class ThirdMaximumNumber {
	
	 public static int thirdMax(int[] nums) {
	        TreeSet<Integer> uniqueNums = new TreeSet<>();
	        for (int num : nums) {
	            uniqueNums.add(num);
	            System.out.println(uniqueNums   + "  uniqueNums.last() :"+uniqueNums.lower(uniqueNums.last()));
	        }
	        if (uniqueNums.size() < 3) {
	            return uniqueNums.last();
	        } else {
	            return uniqueNums.lower(2) ;
	        }
	    }
	 
	 public static void main(String[] args) {
		 int output  = thirdMax(new int[] {1,3 ,4} ) ;
		System.out.println(output);
	}

}



